#include <stdio.h>

int main() {
    int n, n1, digit;
    int sum = 0;

    scanf("%d", &n);
    n1 = n;

    for (; n1 > 0; n1 = n1 / 10) {
        digit = n1 % 10;

        int fact = 1;
        for (int i = 1; i <= digit; i++) {
            fact = fact * i;
        }

        sum = sum + fact;
    }

    if (sum == n) {
        printf("Yes");
    } else {
        printf("No");
    }

    return 0;
}
