#include <stdio.h>
int main() {
    int size;
    scanf("%d", &size);
    int arr[size - 1];
    int actualSum=0;
   
    

    for (int i = 0; i < size - 1; i++) {
        scanf("%d", &arr[i]);
        actualSum += arr[i];
      
    }

    int expectedSum = size * (size + 1) / 2;
    int missing = expectedSum - actualSum;

    printf("%d", missing);
    return 0;
}
