#include <stdio.h>
int main(){
    int size;
    scanf("%d",&size);

    int arr[size];
    for(int i=0;i<size;i++){
        scanf("%d",&arr[i]);
    }

    int increase=1;  
    for(int j=1;j<size;j++){
        if(arr[j]<arr[j-1]){
            increase=0; 
        }
    }

    if(increase){
        printf("Yes\n");
    }
    else{
        printf("No\n");
    }

    return 0;
}
