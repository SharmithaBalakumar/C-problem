#include <stdio.h>
int main(){
    int size;
    scanf("%d",&size);
    int arr[size];
    for(int i=0;i<size;i++){
        scanf("%d",&arr[i]);
    }
    int count1=0;
    int count2=0;
    for(int j=0;j<size;j++){
        if(arr[j]%2==0){
            count1++;
            
        }
        if(arr[j]%2==1){
            count2++;
        }

    }
    printf("Even:%d \n",count1);
    printf("Odd:%d \n",count2);
    return 0;

    
}
