#include <stdio.h>
int main(){
    int size;
    scanf("%d",&size);
    int arr[size];
    for(int i=0;i<size;i++){
        scanf("%d",&arr[i]);
    }
    int max1=arr[0];
    int max2=-1;
    for(int j=1;j<size;j++){
        if(arr[j]>max1){
            max2=max1;
            max1=arr[j];
            
            
        }
        else if(arr[j] < max1 && arr[j] > max2){
            max2=arr[j];
        }
        // printf("%d",max2);
        // return 0;
        
    }
    printf("%d",max2);
    return 0;
    
}
