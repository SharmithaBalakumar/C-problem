#include <stdio.h>
int main(){
    int N;
    scanf("%d",&N);
    int n;
    int i=0;
    int count=0;
    int sum=0;
    while(i<N){
        scanf("%d",&n);
        sum=sum+n;
        if(n>4){
            count++;
        }
        i++;
        
    }
    printf("Total overtime hours:%d \n",sum);
    printf("Burnout days:%d \n",count);
    return 0;

}
