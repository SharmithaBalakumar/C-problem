#include <stdio.h>
int main(){
    int capacity;
    scanf("%d",&capacity);
    int N;
    scanf("%d",&N);
    int n;
    int i=0;
    int count=0;
    int sum=0;
    while(i<N){
        scanf("%d",&n);
        sum=sum+n;
        if(sum>(capacity*90)/100){
            count++;
        }
        i++;
        
    }
    printf("Final occupied beds:%d \n",sum);
    printf("Critical hours:%d \n",count);
    return 0;
