#include <stdio.h>
int main(){
    int N;
    scanf("%d",&N);
    int n;
    int i=0;
    int sum=0;
    int count=0;
    while(i<N){
        scanf("%d",&n);
        sum=sum+n;
        if(n>2){
            count++;
        }
        i++;
    }
    printf("Total Delay:%d \n",sum);
    printf("Delayed Days:%d \n",count);
    return 0;
}
