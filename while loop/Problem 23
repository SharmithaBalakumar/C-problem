#include <stdio.h>

int main() {
    int N;
    scanf("%d", &N);

    int reading;
    int i = 0;
    int unsafeCount = 0;
    int breakdownPoint = -1;  

    while(i < N) {
        scanf("%d", &reading);

        if(reading > 70) {
            unsafeCount++;  
        }

        if(reading >= 100 && breakdownPoint == -1) {
            breakdownPoint = i + 1;  
        }

        i++;
    }

    if(breakdownPoint == -1) {
        printf("Breakdown At Reading: Not Occurred\n");
    } else {
        printf("Breakdown At Reading: %d\n", breakdownPoint);
    }

    printf("Unsafe Readings: %d\n", unsafeCount);

    return 0;
}
